config tlsfragment 'main'
	option enabled '0'
	option port '8080'
	option loglevel 'INFO'
	option mode 'TLSfrag'
	option num_tcp_pieces '8'
	option num_tls_pieces '8'
	option len_tcp_sni '4'
	option len_tls_sni '4'
	option send_interval '0.01'
	option fake_ttl 'query'
	option fake_sleep '0.2'
	option socket_timeout '120'
	option dns_cache '1'
	option dns_cache_ttl '259200'
	option ttl_cache '1'
	option safety_check '0'
	option udp_fake_dns '1'
	option by_sni_first '0'
	option doh_server 'https://cloudflare-dns.com/dns-query?dns='

config domains
	list domain 'google.com'
	list domain 'youtube.com'
	list domain 'facebook.com'
	list domain 'twitter.com'
	list domain 'instagram.com'
	list domain 'github.com'
	list domain 'stackoverflow.com'
